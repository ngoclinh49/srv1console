<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.macpod.srv1console" android:versionName="0.3.2"
	android:versionCode="3">
	<uses-sdk minSdkVersion="2"/>
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".SRV1Console" android:label="@string/app_name"
			android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".SRV1Connect" android:label="@string/srv1connect_name"
			android:screenOrientation="landscape" />
		<activity android:name=".SRV1Settings" android:label="@string/srv1settings_name"
			android:screenOrientation="landscape" />
	</application>
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 